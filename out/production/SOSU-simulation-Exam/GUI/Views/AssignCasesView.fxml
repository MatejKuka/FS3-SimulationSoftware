<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="840.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controllers.AssignCasesController">
   <children>
      <BorderPane layoutX="-6.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox prefHeight="375.0" prefWidth="840.0">
                     <children>
                        <VBox prefHeight="375.0" prefWidth="385.0">
                           <children>
                              <Label alignment="CENTER" prefHeight="90.0" prefWidth="257.0" text="Citizens">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <TableView fx:id="tableViewCitizen" onMouseClicked="#handleClickedCitizen" prefHeight="356.0" prefWidth="281.0">
                                <columns>
                                  <TableColumn fx:id="tcIdCitizen" prefWidth="47.0" text="Id" />
                                  <TableColumn fx:id="tcFNameCitizen" prefWidth="157.0" text="First name" />
                                    <TableColumn fx:id="tcLNameCitizen" prefWidth="160.0" text="Last name" />
                                </columns>
                                 <VBox.margin>
                                    <Insets left="10.0" right="10.0" top="20.0" />
                                 </VBox.margin>
                              </TableView>
                           </children>
                        </VBox>
                        <VBox prefHeight="375.0" prefWidth="412.0">
                           <children>
                              <Label alignment="CENTER" prefHeight="99.0" prefWidth="363.0" text="Students">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <TableView fx:id="tableViewStudents" onMouseClicked="#handleClickedStudent" prefHeight="366.0" prefWidth="540.0">
                                 <columns>
                                    <TableColumn fx:id="tcFNameStudent" prefWidth="186.0" text="First name" />
                                    <TableColumn fx:id="tcLNameStudent" prefWidth="205.0" text="Last name" />
                                 </columns>
                                 <VBox.margin>
                                    <Insets left="10.0" right="10.0" top="20.0" />
                                 </VBox.margin>
                              </TableView>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label prefHeight="32.0" prefWidth="117.0" text="Assign Citizen:" />
                        <VBox alignment="CENTER" prefHeight="99.0" prefWidth="134.0">
                           <children>
                              <Label fx:id="labelFirstname" prefHeight="33.0" prefWidth="127.0" text="label" />
                              <Label fx:id="labelLastName" prefHeight="31.0" prefWidth="129.0" text="label" />
                              <Label fx:id="labelID" prefHeight="30.0" prefWidth="129.0" text="label" />
                           </children>
                        </VBox>
                        <Label prefHeight="33.0" prefWidth="63.0" text="To" />
                        <Label fx:id="labelStudentName" prefHeight="34.0" prefWidth="198.0" text="Label" />
                     </children>
                  </HBox>
                  <VBox alignment="CENTER" prefHeight="143.0" prefWidth="840.0">
                     <children>
                        <Button fx:id="btnAssign" mnemonicParsing="false" onAction="#toAssignCitizen" prefHeight="48.0" prefWidth="99.0" text="Assign">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                        <Label fx:id="labelMessage" alignment="CENTER" prefHeight="28.0" prefWidth="348.0" text="Label" textFill="RED">
                           <VBox.margin>
                              <Insets top="30.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
