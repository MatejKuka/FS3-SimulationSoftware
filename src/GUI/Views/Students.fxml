<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controllers.StudentsController">
   <children>
      <HBox>
         <children>
            <VBox>
               <children>
                  <TableView prefHeight="400.0" VBox.vgrow="ALWAYS">
                    <columns>
                      <TableColumn prefWidth="94.0" text="First Name" />
                      <TableColumn prefWidth="108.0" text="Last Name" />
                        <TableColumn prefWidth="55.0" text="Email" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
            <VBox HBox.hgrow="ALWAYS">
               <children>
                  <HBox spacing="20.0">
                     <children>
                        <TextField />
                        <Button mnemonicParsing="false" text="Button" />
                     </children>
                  </HBox>
                  <ListView prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>

</Pane>
